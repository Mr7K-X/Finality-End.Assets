{
  // Здесь анимации которые есть в .xml файле
  // Не путай местами строки, первое должно быть анимация "idle" (обычный), втором "lose" (проигрыш) и в третьем "win" (победа)
  "animations": [
    "idle анимация здесь",
    "lose анимация здесь",
    "win анимация здесь"
  ],
  //тэги "idle", "lose", "win" не меняй, они работают в системе
  //оффсеты нужны чтобы позиция анимации были в правильном месте
  "animationsOffsets": {
    "idle": [ //первый оффсет горизонтальный (X), второй оффсет вертикальный (Y)
      0,
      0
    ],
    "lose": [
      0,
      0
    ],
    "win": [
      0,
      0
    ]
  }
}
